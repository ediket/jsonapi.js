{
  "name": "backbone.resource",
  "version": "2.0.5",
  "description": "Resource for JSONAPI. Based on Backbone.",
  "keywords": [
    "jsonapi",
    "backbone",
    "resource",
    "restful",
    "model",
    "linkage"
  ],
  "main": "build-npm/jsonapi.js",
  "files": [
    "build-npm"
  ],
  "scripts": {
