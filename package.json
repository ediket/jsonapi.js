    "build": "babel src -d build-npm"
  },
  "author": "ironhee",
  "license": "MIT",
  "dependencies": {
    "backbone": "^1.1.2",
    "jquery": "^2.1.3",
    "lodash": "^3.6.0",
    "node-uuid": "^1.4.3",
    "q": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.1.10",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "chai": "^2.2.0",
    "gulp": "^3.8.11",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-watch": "^4.2.4",
    "karma": "^0.12.31",
    "karma-browserify": "^4.1.2",
    "karma-chrome-launcher": "^0.1.8",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "mocha": "^2.2.4",
    "npm": "^2.8.3",
    "run-sequence": "^1.0.2",
    "sinon": "^1.14.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ironhee/backbone.resource.git"
  },
  "browserify": {
    "debug": true,
    "transform": [
      "babelify"
    ],
    "extensions": [
      ".js"
    ]
